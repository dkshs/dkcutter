{
  "name": "dkcutter",
  "type": "module",
  "version": "4.1.2",
  "packageManager": "pnpm@9.7.0",
  "description": "A command-line utility that creates projects from templates.",
  "author": {
    "name": "DKSHS",
    "url": "https://github.com/dkshs"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkshs/dkcutter.git"
  },
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dkcutter": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "start": "node dist/cli.js",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "changeset": "changeset",
    "release": "changeset version",
    "pub:release": "pnpm build && changeset publish",
    "prepare": "husky"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "execa": "^9.3.0",
    "fs-extra": "^11.2.0",
    "nunjucks": "^3.2.4",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "tsx": "^4.17.0",
    "which": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dkshs/changelog-github": "1.0.6",
    "@dkshs/eslint-config": "2.9.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.15",
    "@types/nunjucks": "^3.2.6",
    "@types/prompts": "^2.4.9",
    "@types/which": "^3.0.4",
    "eslint": "^9.9.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "tsup": "^8.2.4",
    "type-fest": "^4.24.0",
    "typescript": "^5.5.4"
  }
}
