{
  "name": "dkcutter-repo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "description": "A command-line utility that creates projects from templates.",
  "author": {
    "name": "DKSHS",
    "url": "https://github.com/dkshs"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkshs/dkcutter.git"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "eslint . && turbo lint",
    "lint:fix": "eslint . --fix && turbo lint:fix",
    "typecheck": "turbo typecheck",
    "check": "pnpm lint && pnpm typecheck",
    "changeset": "changeset",
    "release": "changeset version",
    "pub:release": "pnpm build && changeset publish",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@dkshs/changelog-github": "1.0.6",
    "@dkshs/eslint-config": "3.4.0",
    "eslint": "^9.18.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "turbo": "^2.3.3",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
