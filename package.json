{
  "name": "dkcutter",
  "version": "4.0.1",
  "description": "A command-line utility that creates projects from templates.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkshs/dkcutter.git"
  },
  "license": "MIT",
  "author": {
    "name": "DKSHS",
    "url": "https://github.com/dkshs"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "dkcutter": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsup --watch",
    "lint": "eslint . && prettier . -c",
    "lint:fix": "eslint --fix . && prettier . -w",
    "check": "pnpm lint && pnpm typecheck",
    "changeset": "changeset",
    "release": "changeset version",
    "pub:release": "pnpm build && changeset publish",
    "prepare": "husky"
  },
  "dependencies": {
    "colorette": "^2.0.20",
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "nunjucks": "^3.2.4",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "tsx": "^4.7.0",
    "which": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@dkshs/changelog-github": "1.0.4",
    "@dkshs/eslint-config": "1.4.0",
    "@types/fs-extra": "^11",
    "@types/node": "^20",
    "@types/nunjucks": "^3",
    "@types/prompts": "^2",
    "@types/which": "^3",
    "eslint": "^8",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "tsup": "^8.0.1",
    "type-fest": "^4.10.2",
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}
