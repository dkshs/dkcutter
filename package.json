{
  "name": "dkcutter",
  "version": "2.4.0",
  "description": "A command-line utility that creates projects from templates.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dkshs/dkcutter.git"
  },
  "license": "MIT",
  "author": {
    "name": "DKSHS",
    "url": "https://github.com/dkshs"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "dkcutter": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsup --watch",
    "lint": "eslint . && prettier . -c",
    "lint:fix": "eslint --fix . && prettier . -w",
    "check": "pnpm lint && pnpm typecheck",
    "changeset": "changeset",
    "release": "changeset version",
    "pub:release": "pnpm build && changeset publish"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cosmiconfig": "^8.3.6",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "nunjucks": "^3.2.4",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "which": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@dkshs/changelog-github": "1.0.2",
    "@dkshs/eslint-config": "1.4.0",
    "@types/fs-extra": "^11",
    "@types/node": "^20",
    "@types/nunjucks": "^3",
    "@types/prompts": "^2",
    "@types/which": "^3",
    "eslint": "^8",
    "tsup": "^7.2.0",
    "type-fest": "^4.7.1",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
